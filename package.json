{
  "name": "meine-seite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "yarn type-check && lint-staged"
